---
// style import
import "@/styles/global.scss";

// layout
import BaseHead from "@layouts/BaseHead.astro";
import BaseLayout from "@layouts/BaseLayout.astro";

// components
import Hero1 from "@components/Hero/Hero1.astro";
import LogoCloud2 from "@components/LogoCloud/LogoCloud2.astro";
import Feature1 from "@components/Feature/Feature1.astro";
import Feature4 from "@components/Feature/Feature4.astro";
import Feature6 from "@components/Feature/Feature6.astro";
import Feature9 from "@components/Feature/Feature9.astro";
import Pricing4 from "@components/Pricing/Pricing4.astro";
import Faq1 from "@components/Faq/Faq1.astro";
import Cta2 from "@components/Cta/Cta2.astro";

// data
import siteData from "@config/siteData.json";
---

<BaseLayout title={siteData.title} description={siteData.description}>
  <Hero1 />
  <LogoCloud2 />
  <Feature6 />
  <Feature1 />
  <Feature4 />
  <Feature9 />
  <Cta2 />
</BaseLayout>
